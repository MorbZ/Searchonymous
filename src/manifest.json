{
	"manifest_version": 2,
	"name": "Searchonymous",
	"version": "1.0.6",
	"description": "__MSG_appDesc__",
	"default_locale": "en",
	"icons": {
		"128": "icon128.png"
	},
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"<all_urls>",
		"storage"
	],
	"background": {
		"scripts": [
			"settings.js",
			"background.js"
		]
	},
	"options_ui": {
		"page": "options.html"
	}
}
